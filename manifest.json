{
    "manifest_version": 3,
    "name": "Trump & Musk Filter",
    "version": "1.0",
    "description": "Removes all mentions and images of Trump and Musk from websites.",
    "permissions": ["storage"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["face-api.min.js", "models/*"],
        "matches": ["<all_urls>"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    }
  }
  